#include <iostream>

using namespace std;

int main() {
    int senha;
    
    // Solicita ao usuário que insira a senha
    cout << "Digite a senha: ";
    cin >> senha;
    
    // Verifica se a senha é igual a 1234
    if (senha == 1234) {
        cout << "ACESSO PERMITIDO" << endl;
    } else {
        cout << "ACESSO NEGADO" << endl;
    }
    
    return 0;
}
